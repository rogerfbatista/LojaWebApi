<configuration>
  <appSettings>
    <add key="owin:appStartup" value="Loja.WebApi.Startup" />
  </appSettings>  
  <connectionStrings>

    <add name="LojaContext" connectionString="Data Source=192.168.1.107;Initial Catalog=Loja;Persist Security Info=True;User ID=sa;Password=123456;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />

